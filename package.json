{
  "name": "relation2",
  "version": "0.0.3",
  "type": "module",
  "bin": "./cli.js",
  "repository": "git@github.com:zjffun/relation.git",
  "author": "Jufeng Zhang <zjffun@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build && npm run test",
    "build": "rm -rf dist && yarn build:cli && yarn build:view",
    "build:cli": "tsc --project tsconfig.build.json",
    "build:view": "cross-env NODE_OPTIONS='--loader ts-node/esm' webpack --config ./src/view/webpack/webpack.prod.ts",
    "watch:cli": "tsc --watch",
    "watch:view": "cross-env NODE_OPTIONS='--loader ts-node/esm' webpack serve --open --config ./src/view/webpack/webpack.dev.ts",
    "test": "cross-env NODE_OPTIONS='--loader ts-node/esm' mocha 'tests/**/*.ts'"
  },
  "files": [
    "cli.js",
    "dist"
  ],
  "dependencies": {
    "commander": "^9.3.0",
    "diff": "^5.0.0",
    "fs-extra": "^10.1.0",
    "remark-parse": "^10.0.1",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/d3": "^7.4.0",
    "@types/diff": "^5.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "d3": "^7.4.4",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "node-sass": "^7.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
